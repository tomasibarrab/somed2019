{
  "name": "harness-smartling-client",
  "version": "0.2.1",
  "description": "Smartling auth, download and upload client to be used with `spur-ioc`.",
  "main": "lib/injector.js",
  "jsnext:main": "./src/injector",
  "publishConfig": {
    "registry": "http://artifactory.otenv.com/artifactory/api/npm/npm-local"
  },
  "scripts": {
    "build": "npm run clean && babel src -d lib --source-maps",
    "clean": "rm -rf lib/ && rm -f test/fixtures/temp/*.zip && rm -rf test/fixtures/temp/temp-* && rm -rf test/fixtures/temp/*.json && rm -rf test/fixtures/temp/foh/*.json && rm -rf test/fixtures/temp/download/*.json",
    "dev": "babel --watch src -d lib",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test-integration": "babel-node --debug node_modules/mocha/bin/_mocha ./test/integration/",
    "test-unit": "babel-node --debug node_modules/mocha/bin/_mocha ./test/unit/",
    "test": "npm run test-unit && npm run test-integration",
    "build-and-test": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/opentable/harness-smartling-client.git"
  },
  "keywords": [
    "harness"
  ],
  "author": {
    "name": "Harness Platform",
    "email": "harness@opentable.com"
  },
  "contributors": [
    {
      "name": "Agustin Colchado",
      "email": "acolchado@opentable.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/opentable/harness-smartling-client/issues"
  },
  "homepage": "https://github.com/opentable/harness-smartling-client",
  "license": "UNLICENSED",
  "dependencies": {
    "del": "^2.2.2",
    "mkdirp": "^0.5.1",
    "spur-ioc": "^0.2.4",
    "unzip2": "^0.2.5",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-opentable": "^4.0.0",
    "eslint-plugin-import": "^1.8.1",
    "harness-common": "^1.0.3",
    "mocha": "^3.2.0",
    "mocha-teamcity-reporter": "^1.1.1",
    "nock": "^9.0.2",
    "sinon": "^1.17.6"
  }
}
