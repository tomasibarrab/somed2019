"use strict";

var _token = require("otkit-colors/token.common");

var _registerTheme = require("../registerTheme");

var _defaultTheme = _interopRequireDefault(require("../../../theme/defaultTheme"));

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

describe('registerTheme', function () {
  describe('Default theme', function () {
    var expectedResult = _defaultTheme.default;
    it('Should return theme object', function () {
      expect((0, _registerTheme.registerTheme)()).toEqual(expectedResult);
    });
  });
  describe('Custom theme', function () {
    it('Should return overwritten theme', function () {
      var customTheme = {
        common: {
          fill: 'red'
        },
        label: {
          fontSize: '20px'
        }
      };
      var controlTheme = (0, _registerTheme.registerTheme)();
      expect(controlTheme.common.fill).toEqual(_token.violetLight);
      expect(controlTheme.label.fontSize).toEqual('13px');
      var overwriteTheme = (0, _registerTheme.registerTheme)(customTheme);
      expect(overwriteTheme.common.fill).toEqual('red');
      expect(overwriteTheme.label.fontSize).toEqual('20px');
    });
  });
});
