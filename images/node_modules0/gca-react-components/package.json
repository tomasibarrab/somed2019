{
  "name": "gca-react-components",
  "version": "1.2.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/opentable/gca-react-components.git"
  },
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "http://artifactory.otenv.com/artifactory/api/npm/npm-local"
  },
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "prettier": "prettier-eslint -l info --write src/components/**/*.js",
    "prebuild": "yarn run prettier && yarn run build:clean && yarn run test",
    "build": "styleguidist build",
    "build:prod": "cross-env NODE_ENV=prod styleguidist build",
    "build:clean": "yarn run test:clean && rimraf ./build",
    "serve": "serve -p 3003 -s styleguide",
    "start": "styleguidist server",
    "clean:all": "yarn run test:clean && yarn run build:clean",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals",
    "lint:js": "yarn run lint:eslint -- . ",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint:staged": "lint-staged",
    "pretest": "yarn run test:clean && yarn run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "*.js": "lint:eslint",
    "*.css": "stylelint",
    "*.scss": "stylelint"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": 0,
      "comma-dangle": 0,
      "import/no-named-as-default": 0,
      "import/prefer-default-export": 0,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "function-paren-newline": [
        "off"
      ],
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/label-has-for": 0,
      "max-len": [
        2,
        {
          "code": 120,
          "tabWidth": 2,
          "ignoreComments": true,
          "ignoreUrls": true
        }
      ],
      "no-confusing-arrow": 0,
      "no-mixed-operators": 0,
      "object-curly-newline": 0,
      "prefer-destructuring": 0,
      "quote-props": 0,
      "react/default-props-match-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-indent": 0,
      "react/jsx-wrap-multilines": 0,
      "react/no-typos": 0,
      "react/require-default-props": 0,
      "space-in-parens": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.dev.babel.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx}",
      "src/utils/validate.test.ts",
      "!src/app.js",
      "!src/Examples/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.jpg$": "<rootDir>/internals/testing/image.js",
      "^.+\\.(css|scss)$": "<rootDir>/internals/testing/cssModule.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": ".test.(js|tsx?)$"
  },
  "devDependencies": {
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.5.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.4",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "immutable": "^3.8.2",
    "imports-loader": "^0.7.1",
    "jest-cli": "^22.4.2",
    "jest-serializer-enzyme": "^1.0.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.6.0",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "postcss-cssnext": "^2.11.0",
    "postcss-focus": "^2.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "^1.1.3",
    "prettier": "1.10.2",
    "prettier-eslint-cli": "^4.7.0",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^15.0.0",
    "react-docgen": "^2.20.1",
    "react-docgen-typescript": "^1.2.3",
    "react-styleguidist": "^6.2.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "redux-observable": "^0.18.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.5.10",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-jest": "^22.4.1",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^14.0.0",
    "uglify-es": "3.3.9",
    "url-loader": "^0.5.7",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "@types/classnames": "^2.2.0",
    "@types/cropperjs": "0.0.26",
    "@types/prop-types": "^15.5.2",
    "@types/react-addons-css-transition-group": "^15.0.2",
    "@types/react-cropper": "^0.10.2",
    "@types/react-dates": "^12.1.6",
    "@types/react-dropzone": "^4.1.0",
    "@types/react-select": "^1.0.46",
    "@types/redux-form": "^7.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "classnames": "^2.2.5",
    "cropperjs": "1.1.1",
    "date-fns": "^1.28.5",
    "final-form": "^4.1.0",
    "libphonenumber-js": "^0.4.43",
    "lodash": "^4.15.0",
    "moment": "^2.18.1",
    "prop-types": "^15.0.0",
    "rc-slider": "8.6.0",
    "react": "^16.2.0",
    "react-addons-shallow-compare": "^15.0.0",
    "react-cropper": "^1.0.0",
    "react-dates": "^12.1.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.1.3",
    "react-final-form": "^3.0.5",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.1.0",
    "react-table": "^6.0.5",
    "react-toggled": "^1.0.2",
    "react-transition-group": "^2.6.1",
    "redux-form": "^7.3.0"
  },
  "resolutions": {
    "@types/react": "16.1.0"
  }
}
