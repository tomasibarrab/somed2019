exports[`PhotoGallery renders the upload interface for restaurants that do not have photos 1`] = `
<div>
  <div
    class="dropdown-message hide"
    role="message"
  >
    <div>
      <div
        class="alert alert--error"
        data-testid="error"
        id="error"
      >
        <div
          class="alert__content"
        >
          <svg
            class="alert__icon alert__icon--error"
            data-testid="iconSvg"
            viewBox="0 0 24 24"
          >
            <use
              xlink:href="#svg-negative"
            />
          </svg>
          <div
            class="alert__message"
          />
        </div>
        <button
          class="touchable"
          data-testid="error-close-bt"
        >
          <svg
            class="alert__close iconLeft"
            data-testid="iconSvg"
            viewBox="0 0 24 24"
          >
            <use
              xlink:href="#svg-cross"
            />
          </svg>
        </button>
      </div>
    </div>
  </div>
  <section
    class="gallery-photos-container"
  >
    <section
      class="gca-panel"
    >
      <header
        class="gca-panel-header"
      >
        <h1>
          Gallery photos
        </h1>
        <div
          class="header-content"
          style="flex: 1 1 auto;"
        >
          <div
            class="photo-tips"
          >
            <button
              class="button photo-tips__action function link() { [native code] } large"
              type="button"
            >
              <span
                class="buttonContent"
                data-testid="buttonContent"
              >
                <span>
                  Photo tips
                </span>
              </span>
            </button>
          </div>
        </div>
      </header>
      <div
        class="gca-panel-body"
      >
        <p
          class="panel-description"
        >
          <span>
            Use gallery photos to enhance your restaurant's OpenTable profile page.
          </span>
        </p>
        <div
          class="photo-upload"
        >
          <img
            alt=""
            class="photo-upload__empty-state-image"
            src="TEST/Empty_States_Photo.svg"
          />
          <h1
            class="photo-upload__header"
          >
            <span>
              Add Photos. Attract more guests.
            </span>
          </h1>
          <p
            class="photo-upload__description"
          >
            <span>
              Photos boost diners’ confidence in booking a reservation. Use photos that best convey your restaurant’s ambiance and guest experience.
            </span>
          </p>
          <button
            class="button photo-upload__upload-button primary large"
            type="button"
          >
            <span
              class="buttonContent"
              data-testid="buttonContent"
            >
              <span>
                Select photos
              </span>
              <input
                data-testid="select-image-file"
                hidden=""
                id="js-select-image-file"
                multiple=""
                type="file"
              />
            </span>
          </button>
          <p
            class="photo-upload__instruction"
          >
            <span>
              or drag and drop files to upload
            </span>
          </p>
        </div>
        <div
          class="guidelines"
        >
          <h2
            class="guidelines__header"
          >
            <span>
              Guidelines
            </span>
          </h2>
          <ul
            class="guidelines__list"
          >
            <li
              class="guidelines__list-item"
            >
              <span>
                Use JPG or PNG formats
              </span>
            </li>
            <li
              class="guidelines__list-item"
            >
              <span>
                Ensure file size is 12MB max
              </span>
            </li>
            <li
              class="guidelines__list-item"
            >
              <span>
                Use minimum dimensions of 512 x 512 (most smartphone photos will work)
              </span>
            </li>
            <li
              class="guidelines__list-item"
            >
              <span>
                Upload up to 50 photos at a time
              </span>
            </li>
            <li
              class="guidelines__list-item"
            >
              <span>
                Don't use your logo or menu
              </span>
            </li>
            <li
              class="guidelines__list-item"
            >
              <span>
                Don't use guests' faces
              </span>
            </li>
          </ul>
        </div>
      </div>
    </section>
    <p
      class="legal-message"
    >
      <span>
        By uploading photos, you agree that we may use these photo(s) to promote your restaurant and us. As a reminder, in our Agreement you represent and warrant that you have the rights to grant this license. Note: photo credits will not be posted.
      </span>
    </p>
  </section>
</div>
`;

