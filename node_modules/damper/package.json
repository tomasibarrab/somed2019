{
  "name": "damper",
  "version": "1.3.13",
  "description": "A collection of Data visualisation components",
  "main": "dist/index.js",
  "author": " <jclements@opentable.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run test",
    "postbuild": "npm run compatible:es5",
    "build": "webpack --config ./internals/webpack.config.js -p",
    "build:watch": "webpack --config ./internals/webpack.config.js -w",
    "compatible:es5": "es-check --module es5 ./dist/index.js",
    "storybook": "start-storybook -p 9090 -c internals/.storybook",
    "storybook:build": "rimraf docs && build-storybook -c internals/.storybook -o docs",
    "lint": "npm run lint:ts",
    "lint:ts": "tslint -c tslint.json --project ./tsconfig.json 'src/**/*.{ts,tsx}'",
    "jsinspect": "jsinspect ./src --threshold 35",
    "test:clean": "rimraf ./coverage",
    "test:only": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test jest --debug=5858 --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "pretest": "npm run test:clean && npm run lint && npm run jsinspect",
    "test": "jest --coverage"
  },
  "publishConfig": {
    "registry": "http://artifactory.otenv.com/artifactory/api/npm/npm-local"
  },
  "pre-commit": [
    "test"
  ],
